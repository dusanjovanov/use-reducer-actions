{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@dusanjovanov/use-reducer-actions",
  "author": "Dusan Jovanov",
  "repository": {
    "url": "https://github.com/dusanjovanov/use-reducer-actions"
  },
  "description": "Simplified useReducer",
  "keywords": [
    "react",
    "hook",
    "use-reducer"
  ],
  "module": "dist/use-reducer-actions.esm.js",
  "size-limit": [
    {
      "path": "dist/use-reducer-actions.cjs.production.min.js",
      "limit": "10 KB"
